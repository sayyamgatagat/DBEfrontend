<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
    <link rel="stylesheet" href="style.css">
    <link rel="stylesheet" href="cust.css">
    <title>Dashboard</title>
</head>
<nav class="navbar navbar-expand-lg navbar-dark">
    <a class="navbar-brand" href="/"><img src="/src/healthcare.png" alt="logo" class="logo"></a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNav">
      <ul class="navbar-nav">
        <li class="nav-item">
          <a class="nav-link" style='color:  rgb(32, 99, 167)' href="customer">Policies</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" style='color:  rgb(32, 99, 167)' href="profile">Profile</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" style='color:  rgb(32, 99, 167)' href="/">Logout</a>
        </li>
      </ul>
    </div>
  </nav>
<body>
<div class="cards">
  <%if(policyTypes != undefined){%>
    <%for(var i=0; i<policyTypes.length ; i++ ){%>
      <div class="container d-flex justify-content-center">
        <figure class="card card-product-grid card-lg"> 
          <a href="#" class="img-wrap" data-abc="true"> <img src="<%=policyTypes[i].img%>"> </a>
            <figcaption class="info-wrap">
                <div class="row ">
                    <div > <h3><a href="#" class="title" data-abc="true"><%=policyTypes[i].pname%></a></h3> <span class="rated"><%=policyTypes[i].type%></span> </div>
                </div>
            </figcaption>
            <div class="bottom-wrap-payment">
                <figcaption class="info-wrap">
                    <div class="row">
                        <div class="desc"> <p href="#" class="desc" data-abc="true"><%=policyTypes[i].desc%></p></div>
                    </div>
                </figcaption>
            </div>
            <div class="bottom-wrap"> 
              <a href="#" class="btn btn-primary float-right" data-abc="true">Get Insured</a>
            </div>
        </figure>
    </div>
    <%}%>
  <%}%>
</div>

</body>